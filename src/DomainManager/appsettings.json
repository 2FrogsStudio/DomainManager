{
  "Bot": {
    "Token": "YOUR_ACCESS_TOKEN_HERE"
  },
  "Serilog": {
    "Using": [
      "Sentry.Serilog"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "DomainManager": "Debug"
      }
    },
    "Enrich": [
      "FromLogContext"
    ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "restrictedToMinimumLevel": "Information"
        }
      },
      {
        "Name": "Sentry",
        "Args": {
          "MinimumBreadcrumbLevel": "Debug",
          "MinimumEventLevel": "Warning"
        }
      }
    ]
  }
}
